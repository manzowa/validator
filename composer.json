{
    "name": "manzowa/manzowa-validator",
    "description": "Manzowa\\Validator is a very small libray PHP",
    "keywords": ["validator"],
    "type": "libray",
    "homepage": "https://github.com/manzowa/validator",
    "license" : "MIT",
    "authors": [
        {
            "name": "Christian SHUNGU",
            "email": "christianshungu@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Manzowa\\Validator\\": "src/"
        }
    },
     "autoload-dev": {
        "psr-4": {
            "Manzowa\\Validator\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "10"
    },
     "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --bootstrap vendor/autoload.php tests/ValidatorTest.php"
    }

}
